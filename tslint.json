{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "jsRules": {
    "object-literal-sort-keys": false
  },
  "rules": {
    "object-literal-sort-keys": false,
    "prefer-for-of": false,
    "no-shadowed-variable": false,
    "ordered-imports": false,
    "curly": false,
    "interface-name": false,
    "member-access": false,
    "comment-format": false,
    "no-console": {
      "severity": "warning"
    }
  }
}